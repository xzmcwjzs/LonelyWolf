                                            } else if (s[0] == "Qw") {
                                                change = "每周一次";
                                            } else if (s[0] == "Biw") {
                                                change = "两周一次";
                                            }
                                            $('#td4_' + i + '').html(change + "，每次" + s[1]);
                                        } else {
                                            change = dats[i - 1].dosage;
                                            $('#td4_' + i + '').html(change);
                                        }
                                    }

                                    if (dats[i - 1].usage != "" && dats[i - 1].usage != null) {
                                        var usage = "";
                                        if (dats[i - 1].usage == "1") {
                                            usage = "口服";
                                        }
                                        else if (dats[i - 1].usage == "2") {
                                            usage = "外用";
                                        }
                                        else if (dats[i - 1].usage == "3") {
                                            usage = "静脉注射";
                                        }
                                        else if (dats[i - 1].usage == "4") {
                                            usage = "肌肉注射";
                                        }
                                        else if (dats[i - 1].usage == "5") {
                                            usage = "皮下注射";
                                        }
                                        else if (dats[i - 1].usage == "6") {
                                            usage = "皮内注射";
                                        }
                                        $('#td5_' + i + '').html(usage);
                                    }
                                    $('#td6_' + i + '').html(dats[i - 1].manu_batch);
                                }
                            }
                        })

                }
            })
        })

    </script>
</head>
